  "scripts": {
    "lint:js": "eslint src --ext .js,.ts",
    "lint:css": "sass-lint -c .sass-lint.yml \"src/**/*.scss\"",
    "lint:html": "htmlhint -c .htmlhintrc src",
    "lint": "npm run lint:js && npm run lint:html && npm run lint:css",
    "start": "webpack serve",
    "build": "rimraf www && webpack --env production",
    "analyze": "rimraf www && webpack --env production --analyze",
    "ionic:serve": "webpack serve",
    "ionic:build": "rimraf www && webpack --env production && ionic deploy manifest",
    "build:prod": "ionic build && ionic cap copy",
    "ionic:android": "ionic cap sync && ionic cap run android -l --host=0.0.0.0",
    "ionic:ios": "ionic cap sync && ionic cap run ios -l --host=0.0.0.0",
    "generate:api": "node ./swagger/codegen.v3.js",
    "codepush:android": "appcenter codepush release-cordova -a tradeexchange/New-Android -d Production -t 1.0.22 -m ",
    "codepush:ios": "appcenter codepush release-cordova -a tradeexchange/New-iOS -d Production -t 1.0.22 -m"
  },
  "postinstall": "npx jetify",
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-device": {},
      "cordova-plugin-secure-storage": {},
      "cordova-plugin-fingerprint-aio": {},
      "cordova-plugin-theme-detection": {}
    },
    "platforms": [
      "android"
    ]
  }
